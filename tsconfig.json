{
  "compilerOptions": {
    "target": "es2021",
    "lib": ["es2021", "es2022"],
    "module": "esnext",
    "moduleResolution": "node",
    "types": ["./worker-configuration.d.ts"],
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
